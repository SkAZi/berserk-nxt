{
  "name": "bersency-next",
  "version": "1.3.2",
  "description": "ККИ Берсерк Nxt",
  "main": "./out/main/index.js",
  "author": "Игорь «SkAZi» Потапов",
  "homepage": "https://berserk-nxt.ru/",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck_full": "npm run typecheck:node && npm run svelte-check",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:web": "vite dev --config vite.config.web.mjs",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:web": "vite build --config vite.config.web.mjs --emptyOutDir && node scripts/shrink-images.mjs",
    "beta:web": "npm run build:web && rsync -Pr ./out_web/ berserk:/var/www/beta.berserk-nxt.ru/html/",
    "release:web": "npm run build:web && rsync -Pr ./out_web/ berserk:/var/www/app.berserk-nxt.ru/html/",
    "build:win": "npm run build && electron-builder --win --x64",
    "build:mac": "npm run build && electron-builder --mac --arm64 --x64",
    "build:lin": "npm run build && electron-builder --linux --x64",
    "build:all": "npm run build && electron-builder --win --linux --x64 && electron-builder --mac --arm64 --x64",
    "publish": "rsync -Pavz --delete --exclude-from='.rsyncignore' ./dist/* berserk:/var/www/updates.berserk-nxt.ru/"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@jamen/svelte-router": "^1.0.0",
    "@picocss/pico": "^2.0.0",
    "adm-zip": "^0.5.10",
    "axios": "^1.6.7",
    "chokidar": "^3.6.0",
    "driver.js": "^1.3.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "fs-extra": "^11.2.0",
    "hammerjs": "^2.0.8",
    "html2canvas": "^1.4.1",
    "jpeg-js": "^0.4.4",
    "jsqr": "^1.4.0",
    "qrcode": "^1.5.3",
    "tar": "^6.2.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@types/node": "^18.19.9",
    "electron": "^28.3.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
    "glob": "^10.3.10",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "sass": "^1.70.0",
    "sharp": "^0.33.3",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "svelte-routing": "^2.12.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-static-copy": "^1.0.1"
  }
}
